{
  "name": "auth0-email-verification",
  "version": "0.0.1",
  "description": "Email Verification",
  "main": "server.js",
  "scripts": {
    "start": "wt-bundle server.js -o ./build/bundle.js && node ./build/bundle.js",
    "bundle": "wt-bundle webtask.js -o ./build/bundle.js",
    "edit": "wt edit --profile demo-workshop-default email-verify",
    "update": "wt update --profile demo-workshop-default email-verify build/bundle.js",
    "deploy": "npm run bundle && npm run update",
    "create": "wt create --name email-verify --profile demo-workshop-default build/bundle.js",
    "logs": "wt logs --profile demo-workshop-default"
  },
  "keywords": [
    "auth0",
    "email",
    "verification",
    "webtask"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:tawawa/auth0-email-verification.git"
  },
  "author": "arcseldon",
  "license": "MIT",
  "dependencies": {
    "async": "^2.1.2",
    "body-parser": "~1.12.4",
    "dotenv": "^2.0.0",
    "express": "4.14.0",
    "lodash": "^4.8.2",
    "morgan": "~1.5.3",
    "nodemailer": "^2.5.0",
    "q": "^1.4.1",
    "ramda": "^0.18.0",
    "request": "^2.67.0"
  },
  "devDependencies": {
    "webtask-tools": "^3.1.1"
  }
}
